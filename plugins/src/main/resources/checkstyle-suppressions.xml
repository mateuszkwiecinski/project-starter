<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

    <suppress checks="MethodName" files=".*src.test.java.*" />
    <suppress checks="MemberName" files=".*src.test.java.*" />
    <suppress checks="MagicNumberCheck" files=".*src.test.java.*" />
    <suppress checks="VisibilityModifier" files=".*src.test.java.*" />
    <suppress checks="RegexpSinglelineJava" files=".*src.test.java.*" />
    <suppress checks="AbbreviationAsWordInNameCheck" files=".*src.test.java.*" />
    <suppress checks="TypeName" files="R.java" />
    <suppress checks="ConstantName" files="R.java" />
    <suppress checks="ConstantName" files="BuildConfig.java" />
    <suppress checks="RegexpSingleline" files=".xml" />
</suppressions>
